{
  "name": "mini-ui",
  "version": "0.0.3",
  "main": "mini-ui.umd.js",
  "module": "mini-ui.es.js",
  "private": true,
  "scripts": {
    "dev": "lerna exec --scope mini-ui yarn docs:dev",
    "test": "npm run lint -- --fix",
    "lint": "eslint ./packages/mini-ui/myui",
    "lint:create": "eslint --init",
    "prepare": "husky install",
    "commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
    "commitlint": "^15.0.0",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/prompt": "^15.0.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-jest": "^27.3.1",
    "commander": "^8.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.13",
    "eslint": "^8.4.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "jest": "^27.3.1",
    "kolorist": "^1.5.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.2",
    "sass": "^1.43.4",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vitepress": "^0.20.1",
    "vitepress-theme-demoblock": "^1.4.1",
    "vue-tsc": "^0.3.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint --fix"
    ],
    "src/**/*.{js,vue,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
